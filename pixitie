/ZXSpectrum-Chargen.pxf/
cell-width 8
cell-height 8
baseline 1
circular-dots
variant-bit Inverse /columns ^FF

charset zx-spectrum.cs

binary-8x8 charcodes 20 .. 7F

/zx-spectrum.cs/
include ascii.cs except 5E, 60

code 5E = U+2191
code 60 = U+00A3
code 7F = U+00A9

/ZXSpectrum-Chargen.8x8.hex/
:1800000000000000000000000010101010001000002424000000000050
