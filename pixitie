/NLQ401-Draft.cs/
include ascii.cs except 7C

code 18 .. 1B = U+00A7, U+00C4, U+00D6, U+00DC
code 1C .. 1F = U+00E4, U+00F6, U+00FC, U+00DF

code 7C = U+00A6 # broken bar
code 7C <- U+007C

code 1F <- U+03B2 # lowercase beta

code 7F -> U+0030 # crossed form of zero

/NLQ401-Draft.pxf/
charset NLQ401-Draft.cs

aspect-ratio 6:5
horizontal-compression 1/2
circular-dots
baseline 5

